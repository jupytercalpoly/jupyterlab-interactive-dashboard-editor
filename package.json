{
  "name": "jupyterlab-interactive-dashboard-editor",
  "version": "0.1.0",
  "description": "Interactively create and customize dashboards in JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupytercalpoly/jupyterlab-interactive-dashboard-editor",
  "bugs": {
    "url": "https://github.com/jupytercalpoly/jupyterlab-interactive-dashboard-editor/issues"
  },
  "license": "BSD-3-Clause",
  "author": "jupytercalpoly",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/jupytercalpoly/jupyterlab-interactive-dashboard-editor.git"
  },
  "scripts": {
    "build": "jlpm run build:lib",
    "build:labextension": "cd jupyterlab_interactive_dashboard_editor && rimraf labextension && mkdirp labextension && cd labextension && npm pack ../..",
    "build:lib": "tsc",
    "build:all": "jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyterlab_interactive_dashboard_editor/labextension",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^2.1.2",
    "@jupyterlab/apputils": "^2.1.1",
    "@jupyterlab/cells": "^2.1.2",
    "@jupyterlab/coreutils": "^4.0.0",
    "@jupyterlab/mainmenu": "^2.2.0",
    "@jupyterlab/notebook": "^2.1.2",
    "@jupyterlab/services": "^5.0.0",
    "@jupyterlab/ui-components": "^2.1.1",
    "@lumino/coreutils": "^1.5.2",
    "@lumino/datastore": "^0.10.3",
    "@lumino/dragdrop": "^1.6.2",
    "@lumino/widgets": "^1.13.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "mkdirp": "^1.0.3",
    "prettier": "2.0.5",
    "rimraf": "^2.6.1",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
